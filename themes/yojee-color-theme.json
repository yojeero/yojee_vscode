			"settings": {
				"fontStyle": "italic",
				"foreground": "#606060"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#8080c0"
			}
		},
